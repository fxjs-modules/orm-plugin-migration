		"@types/path-parse": "^1.0.19",
		"async": "^2.6.2",
		"bluebird": "^3.5.3",
		"path-parse": "^1.0.6"
	},
	"devDependencies": {
		"@fibjs/ci": "^2.2.0",
		"@fibjs/rmdirr": "^2.0.0",
		"@fxjs/orm": "^1.9.4",
		"@types/fibjs": "^0.26.5",
		"@types/lodash": "^4.14.123",
		"cross-env": "^5.2.0",
		"fib-typify": "^0.5.2",
		"lodash": "^4.17.11",
		"shared-examples-for": "^0.1.3",
		"should": "^13.2.3",
